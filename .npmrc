# NPM Configuration for Vercel Deployment
# This file helps resolve dependency conflicts during build

# Use legacy peer deps to resolve conflicts
legacy-peer-deps=true

# Force resolution of peer dependency conflicts
force=true

# Set registry to npmjs
registry=https://registry.npmjs.org/

# Increase timeout for slow connections
timeout=300000

# Set maxsockets for better performance
maxsockets=50

# Enable audit for security
audit=true

# Set fund to false to avoid funding messages
fund=false
